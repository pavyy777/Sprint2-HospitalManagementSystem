@model IEnumerable<HMS.Entity.Models.Doctor>

<div class="row">
    <div class="col-12">
        <h3>Employee Registration History</h3>
        <hr />
        <table class="table table-light table-bordered table-hover">
            <thead>
                <tr>
                    <th>Doctor ID</th>
                    <th>Doctor Name</th>
                    <th> Doctor Qualification</th>
                    <th> Doctor Specialization</th>
                    <th> Consult Fee</th>
                    <th>Email </th>
                    <th>Phone Number </th>
                    <th>Password </th>
                    <th>ReEnter Password</th>
                    <th>Edit</th>
                    <th>Delete</th>
                </tr>
            </thead>
            <tbody>


                @foreach (var loginDoctor in Model)
                {

                    <tr>
                        <td>@loginDoctor.DoctorId</td>
                        <td>@loginDoctor.DoctorName</td>
                        <td>@loginDoctor.DoctorQualification</td>
                        <td>@loginDoctor.DoctorSpecialization</td>
                        <td>@loginDoctor.ConsultFee</td>
                        <td>@loginDoctor.Email</td>
                        <td>@loginDoctor.PhoneNumber</td>
                        <td>@loginDoctor.Password</td>
                        <td>@loginDoctor.ReEnterPassword</td>



                        <td><a asp-controller="Login" asp-action="UpdateDoctor" asp-route-DoctorId="@loginDoctor.DoctorId">Update</a></td>
                        <td><a asp-controller="Login" asp-action="DeleteDoctor" asp-route-DoctorId="@loginDoctor.DoctorId">Delete</a></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
